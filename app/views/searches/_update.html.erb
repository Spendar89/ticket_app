<h2>Total Tickets Analyzed: <%= @total_tickets %>: </h2>
<% @games.each do |game|  %>
<% game_data = game.view_game_data(@price_min, @price_max) %>
<ul class="<%= check_overall_rating(game_data[:overall_rating])[0] %>" description="<%= ticket_summary(game_data) %>">
	<li class ="span12">
		<div class = "date span11">
         <%= game[:date] %>
        </div>
		<div class= "seat_view_div span1">
        <%= link_to(image_tag('binoculars.png', :width => '20px', :height => '20px'), '#', :class => "popover-with-html", :title => "<b>Seat View</b>", "data-placement" => "bottom", "data-content" => "#{image_tag(game_data[:best_ticket].section[:seat_view_url], :width => '400px', :height => '300px', :class => 'img-rounded')}") %>
        </div>
	</li>
	<li class="span12">
		<%= image_tag "team_logos/#{game.team[:name].split(' ')[-1]}.png", :class => "img-rounded", :style => "max-width: 48%;" %>
        <%= image_tag "team_logos/#{game[:opponent].split(' ')[-1]}.png", :class => "img-rounded", :style => "max-width: 48%;" %>
    </li>
    <li class="span11 offset1" style="margin-top: 10%"><%=image_tag "ticket_icon/game_rating.ico", :class => "img-rounded span3"%> <div class="span3" style="font-size: 35px; font-weight: bold; margin-top: 8%; margin-left: -5%; color: grey"><%= game_data[:game_rating] %></div>
    <%=image_tag "ticket_icon/seat_rating.ico", :class => "img-rounded span3"%> <div class="span3" style="font-size: 35px; font-weight: bold; margin-top: 8%; margin-left: -2%; color: grey"><%= game_data[:seat_rating] %></div></li>
    <li class="span6 offset3"><span class="<%= check_overall_rating(game_data[:overall_rating])[1] %>" style="font-size: 50px; padding: 50px 40px 50px 40px; border-radius: 50%;"> <%= game_data[:overall_rating] %></span></li>

        <table class="ticket_info table span12" style="font-size: 20px;">
          <tr class = "ticket_info_values row">
            <td><%= game_data[:best_ticket].section[:name].scan(/\d{1,3}/)[-1] %></td>
            <td><%= game_data[:best_ticket][:row] %></td>
            <td><%= game_data[:best_ticket][:quantity]%></td>
            <td>$<%= game_data[:best_ticket][:price]%></td>
          </tr>
          <tr class="ticket_info_desc row">
            <td>SEC</td>
            <td>ROW</td>
            <td>AVAIL</td>
            <td>PRICE</td>
          </tr>
        </table>

      <li class = "span12">
         <%= link_to "Purchase Ticket", game_data[:best_ticket][:url], :target => 'blank', :class => "btn btn-large btn-block btn-inverse", :style => "margin-top: 5px;" %>
      </li>
</ul>
<% end %>







