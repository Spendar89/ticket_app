<% number_of_games = @games.length %>
<% if number_of_games >= 10 && number_of_games < 15 %>
	<%= javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js" %>
	<%=javascript_tag "$('#line_chart').css('width', '150%')" %>
<% elsif number_of_games >= 15 %>
	<%= javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js" %>		
	<%=javascript_tag "$('#line_chart').css('width', '250%')" %>
<% end %>


<div id="price_chart" class="span12">
	<div id="line_chart_div" class="span12" style="height: 375px; overflow-x:scroll; overflow-y: hidden; position: relative;">
		<div id="line_chart" style="position: relative; top: -10%; height: 420px; width: 100%;" >
			<%= high_chart("line_chart", @line) %>
		</div>
	</div>
</div>

