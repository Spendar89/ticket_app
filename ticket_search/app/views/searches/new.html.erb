<table>
  <%= form_for @search, :method => 'get'  do |f| %>
  <tr>
    <td>Team:</td>
    <td><%= f.text_field :team %></td>
  </tr>
  <tr>
    <td>Date Range Start: </td>
    <td ><%= f.text_field :date_start, :class => 'datepicker'%></td>
  </tr>
  <tr>
    <td>Date Range End: </td>
    <td ><%= f.text_field :date_end, :class => 'datepicker'%></td>
  </tr>
  <tr>
    <td>Price Range: </td>
    <td ><div class = "slider"></div></td>
    <%= f.hidden_field :price_min, :id => "price_min" %>
    <%= f.hidden_field :price_max, :id => "price_max" %>
  </tr>
  <tr>
    <td></td>
    <td id="show_price_min">min: $</td><td id="show_price_max">max: $</td>
  </tr>
    <tr><td><%= f.submit "Find Tickets", :class => 'btn btn-primary' %></td></tr>
  <% end %>
</table>